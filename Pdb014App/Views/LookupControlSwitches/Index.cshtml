@model IEnumerable<Pdb014App.Models.PDB.MeteringPanelModels.LookupControlSwitch>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

    <h2>List of Control Switch</h2>

<p>
    <a asp-action="Create">Add New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.ControlSwitch)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ManufacturesNameCountry)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ManufacturesModelTypeNo)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.ControlSwitch)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ManufacturesNameCountry)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ManufacturesModelTypeNo)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.ControlSwitchId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.ControlSwitchId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.ControlSwitchId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
