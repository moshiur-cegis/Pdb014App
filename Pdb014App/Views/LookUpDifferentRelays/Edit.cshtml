<div class="row">
    <div class="offset-md-3 col-md-6 offset-md-3">

        <fieldset>
            <legend>Update Relay </legend>
        <form asp-action="Edit">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="DifferentRelayId" />
            <div class="form-group">
                <label asp-for="ManufacturersName" class="control-label"></label>
                <input asp-for="ManufacturersName" class="form-control" />
