            </div>
            <div class="form-group">
                <label asp-for="ManufacturersModelNo" class="control-label"></label>
                <input asp-for="ManufacturersModelNo" class="form-control" />
                <span asp-validation-for="ManufacturersModelNo" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="RelayTypeId" class="control-label"></label>
                <select asp-for="RelayTypeId" class="form-control" asp-items="ViewBag.RelayTypeId"></select>
                <span asp-validation-for="RelayTypeId" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-success float-right" />
                <a class="btn btn-success float-left" asp-action="Index">Cencel</a>
            </div>
        </form>
        </fieldset>
    </div>
