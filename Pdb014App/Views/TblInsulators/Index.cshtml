                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.InsulatorToPole.PoleId)
                                </td>
                                @*<td>
                                    <a asp-action="Edit" asp-route-id="@item.InsulatorId">Edit</a> |
                                    <a asp-action="Details" asp-route-id="@item.InsulatorId">Details</a> |
                                    <a asp-action="Delete" asp-route-id="@item.InsulatorId">Delete</a>
                                </td>*@
                            </tr>
                        }
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>

