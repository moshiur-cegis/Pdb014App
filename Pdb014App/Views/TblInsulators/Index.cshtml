                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.FiftyPctImpulseFlashoverWaveNegative)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.PowerFrequencyPuncherVoltage)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.MinimumDryArchingDistance)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.PowerFrequencyTestVoltageRmsToGround)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.MinimumRadioInfluenceVoltageRIVAt1000KcInMicroVolt)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.MinimumNeckDiameter)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.MinimumDiameterOfInsulator)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.MinimumHeightOfTheInsulator)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.MinimumGroveDiameter)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.MinimumMechanicalFailingLoad)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.InsulatorToLookUpCondition.Name)
                                </td>
