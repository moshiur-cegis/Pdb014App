                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.NominalSystemVoltage)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.InsulatorType.InsulatorTypeName)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.MaximumSystemVoltage)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.TypeOfSystem)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.InsulatorVoltageClass)
                                </td>
                                <td>
