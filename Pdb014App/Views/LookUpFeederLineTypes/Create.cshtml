@model Pdb014App.Models.PDB.SubstationModels.LookUpFeederLineType

@{
    ViewData["Title"] = "Create";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


@*<h4>Add Feeder Line Type</h4>
<hr />*@
<div class="row">
    <div class="offset-md-3 col-md-6 offset-md-3">
        
        <fieldset>
            <legend>Add Feeder Line Type</legend>
            <form asp-action="Create">
                <div asp-validation-summary="ModelOnly" class="text-danger"></div>

                <div class="form-group">
                    <label asp-for="FeederLineTypeId" class="control-label">Feeder Line Type Id</label>
                    <input asp-for="FeederLineTypeId" class="form-control" />
                    <span asp-validation-for="FeederLineTypeId" class="text-danger"></span>
                </div>
                <div class="form-group">
                    <label asp-for="FeederLineTypeName" class="control-label">Feeder Line Type Name</label>
                    <input asp-for="FeederLineTypeName" class="form-control" />
                    <span asp-validation-for="FeederLineTypeName" class="text-danger"></span>
                </div>
                <div class="form-group">
                    <input type="submit" value="Create" class="btn btn-success float-right" />
                    <a class="btn btn-success float-left" asp-action="Index">Back</a>
                </div>
            </form>
            </fieldset>
</div>
</div>

