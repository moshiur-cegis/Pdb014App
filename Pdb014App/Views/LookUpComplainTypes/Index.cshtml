@model IEnumerable<Pdb014App.Models.PDB.LookUpComplainType>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.ComplainType)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.ComplainType)
                </td>
                <td>
                    <a asp-action="Edit" asp-route-id="@item.ComplainTypeId">Edit</a> |
                    <a asp-action="Details" asp-route-id="@item.ComplainTypeId">Details</a> |
                    <a asp-action="Delete" asp-route-id="@item.ComplainTypeId">Delete</a>
                </td>
            </tr>
        }
    </tbody>
</table>
