@model Pdb014App.Models.PDB.ConductorModels.TblConductor

@{
    ViewData["Title"] = "Create";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@section Styles
    {

    <link href="~/lib/bootstrap/dist/css/bootstrap-select.css" rel="stylesheet" />
}


<div class="row">
    <div class="offset-md-3 col-md-6 offset-md-3">
        
        <fieldset>
            <legend>Add Conductor Information</legend>
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            @*<div class="form-group">
                <label asp-for="ConductorId" class="control-label"></label>
                <input asp-for="ConductorId" class="form-control" />
            </div>*@
            <div class="form-group">
                <label asp-for="ConductorTypeId" class="control-label"></label>
                <select asp-for="ConductorTypeId" class="form-control" asp-items="ViewBag.ConductorTypeId">
                    <option>--Select--</option>
                </select>
            </div>
            <div class="form-group">
                <label asp-for="ConductorName" class="control-label"></label>
                <input asp-for="ConductorName" class="form-control" />
                <span asp-validation-for="ConductorName" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Standard" class="control-label"></label>
                <input asp-for="Standard" class="form-control" />
                <span asp-validation-for="Standard" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Installation" class="control-label"></label>
                <input asp-for="Installation" class="form-control" />
                <span asp-validation-for="Installation" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Material" class="control-label"></label>
                <input asp-for="Material" class="form-control" />
