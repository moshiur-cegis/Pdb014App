            <div class="form-group">
                <label asp-for="NeutralSize" class="control-label"></label>
                <input asp-for="NeutralSize" class="form-control" />
                <span asp-validation-for="NeutralSize" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="AssemblyCode" class="control-label"></label>
                <input asp-for="AssemblyCode" class="form-control" />
                <span asp-validation-for="AssemblyCode" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="NominalVoltage" class="control-label"></label>
                <input asp-for="NominalVoltage" class="form-control" />
                <span asp-validation-for="NominalVoltage" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="PhaseOrientation" class="control-label"></label>
                <input asp-for="PhaseOrientation" class="form-control" />
                <span asp-validation-for="PhaseOrientation" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="InstallDate" class="control-label"></label>
                <input asp-for="InstallDate" class="form-control" />
                <span asp-validation-for="InstallDate" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="ConductorCrossSection" class="control-label"></label>
                <input asp-for="ConductorCrossSection" class="form-control" />
                <span asp-validation-for="ConductorCrossSection" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="ConductorCapacityAmp" class="control-label"></label>
                <input asp-for="ConductorCapacityAmp" class="form-control" />
                <span asp-validation-for="ConductorCapacityAmp" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="TotalSanctionedLoadFromTheFeeder" class="control-label"></label>
                <input asp-for="TotalSanctionedLoadFromTheFeeder" class="form-control" />
                <span asp-validation-for="TotalSanctionedLoadFromTheFeeder" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="PoleId" class="control-label"></label>
                <select asp-for="PoleId" data-live-search="true" title="Select Pole Id" class="form-control selectpicker" asp-items="ViewBag.PoleId"></select>

            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-success float-right" />
                <a class="btn btn-success float-left" asp-action="Index">Back</a>
            </div>
        </form>
        </fieldset>
    </div>
</div>




