            <div class="form-group">
                <label asp-for="NumberOrDiameterOfAluminum" class="control-label"></label>
                <input asp-for="NumberOrDiameterOfAluminum" class="form-control" />
                <span asp-validation-for="NumberOrDiameterOfAluminum" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="CrossSectionalAreaOfConducto" class="control-label"></label>
                <input asp-for="CrossSectionalAreaOfConducto" class="form-control" />
                <span asp-validation-for="CrossSectionalAreaOfConducto" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="NominalAluminumCrossSectionalArea" class="control-label"></label>
                <input asp-for="NominalAluminumCrossSectionalArea" class="form-control" />
                <span asp-validation-for="NominalAluminumCrossSectionalArea" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="NumberOrdiameterOfSteel" class="control-label"></label>
                <input asp-for="NumberOrdiameterOfSteel" class="form-control" />
                <span asp-validation-for="NumberOrdiameterOfSteel" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="NominalSteelCrossSectionalArea" class="control-label"></label>
                <input asp-for="NominalSteelCrossSectionalArea" class="form-control" />
                <span asp-validation-for="NominalSteelCrossSectionalArea" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="WeightOfConductor" class="control-label"></label>
                <input asp-for="WeightOfConductor" class="form-control" />
                <span asp-validation-for="WeightOfConductor" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="CodeName" class="control-label"></label>
                <input asp-for="CodeName" class="form-control" />
                <span asp-validation-for="CodeName" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="MaximumDcResistanceOfConductorAt20DegC" class="control-label"></label>
                <input asp-for="MaximumDcResistanceOfConductorAt20DegC" class="form-control" />
