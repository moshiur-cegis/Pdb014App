@model IEnumerable<Pdb014App.Models.PDB.ServicePointModels.LookUpConsumerType>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

    <h2>List of Consumer Type</h2>

<p>
    <a asp-action="Create">Add New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.ConsumerTypeName)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.ConsumerTypeName)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.ConsumerTypeId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.ConsumerTypeId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.ConsumerTypeId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
